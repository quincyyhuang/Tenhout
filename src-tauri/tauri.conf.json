{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Tenhout",
  "version": "0.2.2",
  "identifier": "com.tenhout.tenhout",
  "build": {},
  "app": {
    "withGlobalTauri": true,
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": {
      "../plugins/**/*": "plugins/"
    }
  }
}
